{
    "name": "dangdungcntt/nddapp-cli",
    "description": "NDDApp CLI.",
    "keywords": ["nddapp", "cli"],
    "homepage": "https://nddapp.com",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/dangdungcntt/nddapp-cli/issues",
        "source": "https://github.com/dangdungcntt/nddapp-cli"
    },
    "authors": [
        {
            "name": "Dung Nguyen Dang",
            "email": "dangdungcntt@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.4",
        "illuminate/http": "^9.0",
        "illuminate/view": "^9.0",
        "laravel-zero/framework": "^9.0",
        "laravel-zero/phar-updater": "^1.2",
        "nunomaduro/termwind": "^1.3"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.4",
        "pestphp/pest": "^1.21.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["nddapp-cli"]
}
